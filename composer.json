{
    "name": "faustbrian/laravel-blade-extensions",
    "description": "Extensions and Directives for Laravel Blade",
    "keywords": ["laravel", "framework", "Laravel-Blade-Extensions", "Laravel Blade Extensions", "Brian Faust", "faustbrian"],
    "license": "MIT",
    "authors": [{
        "name": "Brian Faust",
        "email": "hello@brianfaust.de",
        "homepage": "https://brianfaust.de",
        "role": "Developer"
    }],
    "require": {
        "php": "^5.6 || ^7.0",
        "illuminate/view": "5.1.* || 5.2.* || 5.3.*",
        "illuminate/support": "5.1.* || 5.2.* || 5.3.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.1",
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "^5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "BrianFaust\\Blade\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BrianFaust\\Tests\\Blade\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
